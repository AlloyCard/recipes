{
    "name": "Restaurant Tip Template",
    "components": {
        "mainView": {
            "type": "View",
            "props": {
                "style": {
                    "flexDirection": "row",
                    "justifyContent": "space-between"
                }
            },
            "children": {
                "totalPanel": {
                    "type": "View",
                    "props": {
                        "style": {
                            "alignItems": "center"
                        }
                    },
                    "children": {
                        "totalLabel": {
                            "type": "Text",
                            "props": {
                                "style": {
                                    "fontSize": 20
                                },
                                "label": "Total"
                            }
                        },
                        "totalAmount": {
                            "type": "Text",
                            "props": {
                                "style": {
                                    "fontSize": 20
                                },
                                "label": "$$total"
                            }
